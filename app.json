{
  "expo": {
    "name": "MOOD.ai",
    "slug": "mood",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "moodai",
    "backgroundColor": "#000000",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.moodplatforms.moodai"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.moodplatforms.moodai"
    },
    "androidStatusBar": {
      "backgroundColor": "#000000",
      "barStyle": "light-content",
      "translucent": true
    },
    "web": {
      "favicon": "./assets/favicon.ico",
      "bundler": "metro"
    },
    "plugins": ["expo-router", "expo-localization"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1170be0c-9524-45c1-ab2d-81d70e3fe0ab"
      }
    }
  }
}
